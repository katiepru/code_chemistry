<!DOCTYPE html>
<html>
  <body>
	  <div class="row">
		  <div class="well">
			  <%= form_tag("/project/create", :method => "post") do %>
				  <%= label_tag(:name, "Enter Name:") %>
				  <%= text_field_tag(:name) %>
				  <%= submit_tag("Submit") %>
				  <br/>
				  <%= label_tag(:url, "Enter GitHub URL:") %>
				  <%= text_field_tag(:url) %>
				  <%= submit_tag("Submit") %>
			  <% end %>
		  </div>
	  </div>
  </body>
</html>
