<!DOCTYPE html>
<html>
  <body>
	  <div class="row">
      <div class="span 9 offset6">
        <div class="well">
          <%= form_tag("/project/create", :method => "get") do %>
            <%= label_tag(:name, "Enter Name:") %>
            <%= text_field_tag :name, params[:name] %>
            <%= label_tag(:url, "Enter GitHub URL:") %>
            <%= text_field_tag :url, params[:url] %>
            <br/>
            <%= submit_tag("Submit", :class => 'btn') %>
          <% end %>
        </div>
      </div>
	  </div>
  </body>
</html>
